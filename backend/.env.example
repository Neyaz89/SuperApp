# SuperApp Backend Environment Variables

# Server Configuration
PORT=3000
NODE_ENV=production

# Proxy Configuration (Optional - for high traffic)
USE_PROXY=false
PROXY_LIST=

# Example proxy list (comma-separated):
# PROXY_LIST=http://user:pass@proxy1.com:8080,http://proxy2.com:8080,socks5://proxy3.com:1080

# API Keys (Optional - for premium features)
# YOUTUBE_API_KEY=your_youtube_api_key_here
# COBALT_API_KEY=your_cobalt_api_key_here

# Rate Limiting (Optional)
RATE_LIMIT_ENABLED=false
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# Cache Configuration
CACHE_TTL=3600
CACHE_ENABLED=true

# Logging
LOG_LEVEL=info
LOG_EXTRACTION_DETAILS=true

# Timeout Configuration (milliseconds)
EXTRACTION_TIMEOUT=20000
API_TIMEOUT=15000

# Feature Flags
ENABLE_CUSTOM_EXTRACTORS=true
ENABLE_YTDLP_FALLBACK=true
ENABLE_COBALT_FALLBACK=true
ENABLE_SAVEFROM_FALLBACK=true

# Cookie Management
COOKIES_ENABLED=true
COOKIES_PATH=./cookies

# Security
CORS_ORIGIN=*
ALLOWED_DOMAINS=

# Performance
MAX_CONCURRENT_EXTRACTIONS=10
QUEUE_ENABLED=false
