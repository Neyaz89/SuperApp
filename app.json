{
  "expo": {
    "name": "SuperHub",
    "slug": "superapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "scheme": "superhub",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#A78BFA"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.superhub.media",
      "icon": "./assets/logo.png"
    },
    "android": {
      "icon": "./assets/logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#A78BFA"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES"
      ],
      "package": "com.superhub.media",
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true
    },
    "plugins": [
      "expo-router",
      "expo-file-system",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow SuperHub to save downloaded media.",
          "savePhotosPermission": "Allow SuperHub to save media to your gallery."
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4846583305979583~7315110438",
          "iosAppId": "ca-app-pub-4846583305979583~7315110438"
        }
      ],
      [
        "react-native-compressor",
        {
          "enableBackgroundMode": true
        }
      ]
    ],
    "sdkVersion": "54.0.0",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ec4e9340-3d51-4c13-b7a8-6a87eaeeb60b"
      }
    },
    "owner": "neyaz12"
  }
}
